{
  "name": "app",
  "version": "0.1.0",
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.5.2",
    "electron-store": "^6.0.1",
    "fontsource-roboto": "^3.0.3",
    "mdi-react": "^7.3.0",
    "moment": "^2.29.1",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-bootstrap": "^1.3.0",
    "react-bootstrap-table-next": "^4.0.3",
    "react-bootstrap-table2-filter": "^1.3.3",
    "react-bootstrap-table2-paginator": "^2.1.2",
    "react-bootstrap-table2-toolkit": "^2.1.3",
    "react-burger-menu": "^2.9.0",
    "react-date-picker": "^8.0.3",
    "react-datepicker": "^3.3.0",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "recharts": "^1.8.5",
    "typescript": "^3.8.3",
    "uuid": "^8.3.1"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "yarn build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/lowdb": "^1.0.9",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.51",
    "@types/react-bootstrap": "^0.32.24",
    "@types/react-bootstrap-table-next": "^4.0.8",
    "@types/react-bootstrap-table2-filter": "^1.3.0",
    "@types/react-bootstrap-table2-paginator": "^2.1.1",
    "@types/react-bootstrap-table2-toolkit": "^2.1.4",
    "@types/react-burger-menu": "^2.6.2",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/recharts": "^1.8.16",
    "@types/uuid": "^8.3.0",
    "build": "^0.1.4",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.2.0",
    "concurrently": "^5.3.0",
    "devtron": "^1.4.0",
    "electron": "10.1.3",
    "electron-builder": "22.8.1",
    "wait-on": "^5.2.0"
  },
  "build": {
    "appId": "com.example.electron-cra",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
